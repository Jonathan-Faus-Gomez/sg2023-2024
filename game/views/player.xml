<odoo>
    <data>
vesprada


        <record model="ir.ui.view" id="game_player_tree_view">
            <field name="name">game.player.tree</field>
            <field name="model">game.player</field>
            <field name="arch" type="xml">
                <tree string="Jugadores">
                    <field name="name"/>
                    <field name="level"/>
                    <field name="dinos"/>
                    <field name="edificios"/>
                    <field name="oro"/>
                    <field name="carne"/>
                    <field name="vegetal"/>
                </tree>
            </field>
        </record>


        <record model="ir.ui.view" id="game_player_form_view">
            <field name="name">game.player.form</field>
            <field name="model">game.player</field>
            <field name="arch" type="xml">
                <form string="Jugador">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="edificios"/>
                            <field name="dinos"/>
                            <field name="oro"/>
                            <field name="carne"/>
                            <field name="vegetal"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <record model="ir.ui.view" id="game_player_kanban_view">
            <field name="name">game.player.kanban</field>
            <field name="model">game.player</field>
            <field name="arch" type="xml">
                <kanban>
                    <templates>
                        <t t-name="kanban-box">
                            <div class="oe_kanban_title">
                                <strong >
                                    <field name="name"/>
                                </strong>
                            </div>
                            <div class="oe_kanban_details">
                                <field name="level"/>
                                <!--
                                <img class="oe_kanban_image"
                                        t-att-alt="record.name.value"
                                        t-att-src="kanban_image('game.player', 'foto', record.id.value)" />
                                 -->
                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>

    </data>
</odoo>