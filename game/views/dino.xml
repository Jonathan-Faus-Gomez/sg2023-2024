<odoo>
    <data>

        <record model="ir.ui.view" id="game_dino_tree_view">
            <field name="name">game.dino.tree</field>
            <field name="model">game.dino</field>
            <field name="arch" type="xml">
                <tree string="Dinos">
                    <field name="name"/>
                    <field name="tipo"/>
                    <field name="vida"/>
                    <field name="ataque"/>
                    <field name="player"/>
                    <field name="tamany"/>
                    <field name="ocupa"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="game_dino_form_view">
            <field name="name">game.dino.form</field>
            <field name="model">game.dino</field>
            <field name="arch" type="xml">
                <form string="Dino">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="level"/>
                            <field name="tipo"/>
                            <field name="player"/>
                            <field name="tamany"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="game_dino_kanban_view">
    <field name="name">game.dino.kanban</field>
    <field name="model">game.dino</field>
    <field name="arch" type="xml">
        <kanban>
            <templates>
                <t t-name="kanban-box">
                    <div class="oe_kanban_image">
                        <img t-att-src="'/web/image/game.dino/' + record.id + '/imagen'"/>
                    </div>
                    <div class="oe_kanban_title">
                        <strong>
                            <field name="name"/>
                        </strong>
                    </div>
                    <div class="oe_kanban_details">
                        <field name="level"/>
                    </div>
                </t>
            </templates>
        </kanban>
    </field>
</record>



    </data>
</odoo>