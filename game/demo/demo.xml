<odoo>
    <data>

        <record id="player_demo1" model="res.partner">
            <field name="name">Antonio</field>
            <field name="level">3</field>
            <field name="carne">5000</field>
            <field name="vegetal">4000</field>
            <field name="oro">12000</field>
            <field name="dinos" eval="[(0, 0, {'name': 'Raptor', 'level': 4, 'tipo': '1', 'tamany': '2'})]"/>
            <field name="edificios" eval="[(0, 0, {'name': 'Almacen', 'tipo': '1', 'level': 2})]"/>
        </record>

        <record id="player_demo2" model="res.partner">
            <field name="name">Mar√≠a</field>
            <field name="level">2</field>
            <field name="carne">3000</field>
            <field name="vegetal">3500</field>
            <field name="oro">8000</field>
            <field name="dinos" eval="[(0, 0, {'name': 'Triceratops', 'level': 3, 'tipo': '2', 'tamany': '3'})]"/>
            <field name="edificios" eval="[(0, 0, {'name': 'Torre', 'tipo': '2', 'level': 1})]"/>
        </record>

        <record id="batalla_demo1" model="game.batalla">
            <field name="name">BatallaDemo1</field>
            <field name="inicio" eval="(datetime.datetime.now() - timedelta(days=1)).strftime(DEFAULT_SERVER_DATETIME_FORMAT)"/>
            <field name="player1" ref="player_demo1"/>
            <field name="player2" ref="player_demo2"/>
        </record>

    </data>
</odoo>
